@using System.Linq
@using Easy.CMS.Section.Models
@model SectionGroup

<div class="section-group-default">
    @foreach (SectionContent content in Model.SectionContents.OrderBy(m => m.Order))
    {
        Html.RenderPartial("SectionPartial." + ((SectionContent.Types)content.SectionContentType), content);
    }
</div>
