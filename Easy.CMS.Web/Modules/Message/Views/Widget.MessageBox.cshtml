@using Easy.CMS.Message.Models
@model IEnumerable<MessageEntity>

<div class="message-box">
    @foreach (var item in Model)
    {
        <div class="bs-callout bs-callout-info">
            <p>
                <strong>
                    @item.PostMessage
                </strong>                
            </p>
            <div class="sumary text-muted">
                @Html.Raw(item.Reply)
            </div>
        </div>
        <br />
    }
</div>